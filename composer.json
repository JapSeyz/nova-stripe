{
    "name": "tightenco/nova-stripe",
    "description": "A tool to create a quick Stripe dashboard in your Laravel Nova admin panels",
    "keywords": [
        "tightenco",
        "laravel",
        "nova",
        "stripe"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Samantha Geitz",
            "email": "samantha@tighten.co"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "stripe/stripe-php": ">=5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.*",
        "orchestra/testbench": ">=3.6.x-dev",
        "laravel/framework": ">=6.20.26",
        "laravel/nova": "*@dev"
    },
    "autoload": {
        "psr-4": {
            "Tightenco\\NovaStripe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tightenco\\NovaStripe\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tightenco\\NovaStripe\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
